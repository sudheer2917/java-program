class relational{
   public static void main(String[] args)
    {
       int a=7,b=11;
        System.out.println("a is"+a+"and b is"+b);
        System.out.println(a==b);
        System.out.println(a!=b);
        System.out.println(a>b);
        System.out.println(a<b);
        System.out.println(a>=b);
        System.out.println(a<=b);
    }
  }
output:a is7and b is11
false
true
false
true
false
true
