class logical{
public static void main(String[] args)
{
  System.out.println((5>3)&&(8>5));
  System.out.println((5>3)&&(8<5));
  System.out.println((5<3)||(8>5));
  System.out.println((5>3)||(8<5));
  System.out.println((5<3)||(8<5));
  System.out.println(!(5==3));
  System.out.println(!(5>3));
    }
}
out put:
true
false
true
true
false
true
false
